identityfile        /home/s.seletskiy/.ssh/id_rsa

host *
   controlmaster    auto
   controlpersist   1h
   controlpath      ~/.ssh/connections/%r_%h_%p
   compression      no
   sendenv          TERM

host *.d *.t*
	user            root
	identityfile    ~/.ssh/id_rsa_env

host tx
    hostname        chef.ngs.local
    user            deployer

host task*
    user            ngs

host git
    user git

host baranov
    user            guest
